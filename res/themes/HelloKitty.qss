.log-server {
  background-color: transparent;
  color: rgba(255, 20, 147, 1.0); /* Deep pink for server messages */
  font-weight: bold;
}

.log-channel {
  background-color: transparent;
  color: rgba(255, 182, 193, 1.0); /* Light pink for channels */
  font-weight: bold;
}

.log-privilege {
  background-color: transparent;
  color: rgba(199, 21, 133, 1.0); /* Medium violet red for privileges */
  font-weight: bold;
}

.log-target {
  background-color: transparent;
  color: rgba(255, 105, 180, 1.0); /* Hot pink for targets */
  font-weight: bold;
}

.log-source {
  background-color: transparent;
  color: rgba(255, 105, 180, 1.0); /* Hot pink for sources */
  font-weight: bold;
}

QListView#qlwIcons {
  padding: 0;
  background-color: transparent;
  border: 0;
  font-size: 12pt;
  min-width: 165%;
