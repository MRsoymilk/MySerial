  margin-top: 12px;
}

QListView#qlwIcons::item {
  margin-bottom: 1px;
  padding: 5px 7px;
}

QListView#qlwIcons::item:hover {
  border-color: rgba(255, 209, 220, 1.0);
  background-color: rgba(255, 209, 220, 0.8);
}

QListView#qlwIcons::item:selected {
  background-color: rgba(255, 255, 255, 0.8);
  border: 1px solid rgba(255, 193, 204, 1.0);
}

QListView#qlwIcons::item:focus {
  background-color: rgba(255, 209, 220, 0.8);
  border: 1px solid rgba(255, 105, 180, 1.0);
}

QSlider {
